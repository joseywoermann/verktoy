{
    "name": "verktoy",
    "version": "1.0.0",
    "description": "verktøy is a utility bot and successor of navnløs.",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "ts:build": "tsc",
        "ts:dev": "tsc --watch",
        "dev": "nodemon .",
        "prod": "node .",
        "start": "npm run ts:build && npm run prod",
        "deploy": "node dist/deploy.js",
        "format": "prettier . --write",
        "clean:all": "rmdir /S dist && rmdir /S node_modules",
        "clean:dist": "rmdir /S dist",
        "clean:npm": "rmdir /S node_modules"
    },
    "author": "Josey C. Wörmann (github.com/joseywoermann)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joseywoermann/verktoy.git"
    },
    "bugs": {
        "url": "https://github.com/joseywoermann/verktoy/issues"
    },
    "imports": {
        "#buttons": "./dist/components/buttons/__exports.js",
        "#selects": "./dist/components/selects/__exports.js",
        "#commands": "./dist/commands/__exports.js",
        "#util": "./dist/util/__exports.js"
    },
    "dependencies": {
        "@josey/logger": "^2.0.1",
        "discord.js": "^13.6.0",
        "dotenv": "^10.0.0",
        "mathjs": "^10.1.0",
        "node-fetch": "^3.2.0",
        "qrcode": "^1.5.0",
        "statcord.js": "^3.3.0"
    },
    "devDependencies": {
        "@types/node": "^17.0.8",
        "@types/qrcode": "^1.4.2",
        "nodemon": "^2.0.20",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    }
}
