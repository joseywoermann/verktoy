{
    "name": "verktoy",
    "version": "1.0.0",
    "description": "verktøy is a utility bot and successor of navnløs.",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "ts:build": "tsc",
        "ts:dev": "tsc --watch",
        "dev": "nodemon .",
        "start": "node .",
        "deploy": "node dist/deploy.js",
        "format": "prettier . --write",
        "clean:all": "rmdir /S dist && rmdir /S node_modules",
        "clean:dist": "rmdir /S dist",
        "clean:npm": "rmdir /S node_modules"
    },
    "author": "Josey C. Wörmann (github.com/joseywoermann)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joseywoermann/verktoy.git"
    },
    "bugs": {
        "url": "https://github.com/joseywoermann/verktoy/issues"
    },
    "imports": {
        "#buttons/*": "./dist/components/buttons/*",
        "#selects/*": "./dist/components/selects/*",
        "#commands/*": "./dist/commands/*",
        "#util/*": "./dist/util/*"
    },
    "dependencies": {
        "@josey/logger": "^2.0.1",
        "discord.js": "^13.5.1",
        "dotenv": "^10.0.0",
        "mathjs": "^10.0.2",
        "node-fetch": "^3.1.0",
        "statcord.js": "^3.3.0",
        "urban-dictionary": "^3.0.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.8",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    }
}
